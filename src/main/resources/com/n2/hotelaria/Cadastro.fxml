<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="rootPane" prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.CadastroController">
   <children>
      <Label layoutX="66.0" layoutY="30.0" text="Cadastre-se" />
      <Label layoutX="66.0" layoutY="67.0" text="Nome Completo:" />
      <TextField fx:id="nomeCompletoField" layoutX="66.0" layoutY="86.0" prefWidth="200.0" />
      <Label layoutX="66.0" layoutY="125.0" text="Email:" />
      <TextField fx:id="emailField" layoutX="66.0" layoutY="144.0" prefWidth="200.0" />
      <Label layoutX="66.0" layoutY="183.0" text="CPF:" />
      <TextField fx:id="cpfField" layoutX="66.0" layoutY="202.0" prefWidth="200.0" />
      <Label layoutX="66.0" layoutY="241.0" text="Data de Nascimento:" />
      <TextField fx:id="dataNascimentoField" layoutX="66.0" layoutY="260.0" prefWidth="200.0" />
      <Label layoutX="66.0" layoutY="299.0" text="Senha:" />
      <PasswordField fx:id="senhaField" layoutX="66.0" layoutY="318.0" prefWidth="200.0" />
      <Button fx:id="voltarButton" layoutX="206.0" layoutY="376.0" mnemonicParsing="false" onAction="#handleVoltar" text="Voltar" />
      <Button fx:id="cadastrarButton" layoutX="106.0" layoutY="376.0" mnemonicParsing="false" onAction="#handleCadastro" text="Cadastrar" />
